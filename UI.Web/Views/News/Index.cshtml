@model IEnumerable<DataAccess1.News>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.users.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.users1.password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.creationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.updateDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.users.password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.users1.password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.creationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.updateDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.title }) |
            @Html.ActionLink("Details", "Details", new { id=item.title }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.title })
        </td>
    </tr>
}

</table>
